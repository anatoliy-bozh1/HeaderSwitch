{
  "name": "headerswitch",
  "displayName": "HeaderSwitch",
  "description": "Visual Studio Code extension to quickly and automatically switch between C++ source (.cpp) and header (.h/.hpp) files.",
  "version": "1.0.0",
  "publisher": "anatoliy-bozh1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onCommand:headerswitch.switch"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "headerswitch.switch",
        "title": "Switch Header/Source"
      }
    ]
  }
}